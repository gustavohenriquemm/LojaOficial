{
  "name": "loja-oficial-backend",
  "version": "1.0.0",
  "description": "Backend para integração com Mercado Pago",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node server-test.js",
    "init": "node init.js",
    "health-check": "node -e \"const http=require('http'); http.get('http://localhost:'+process.env.PORT+'/health', (r)=>{r.statusCode===200?console.log('✅'):process.exit(1);}).on('error',()=>process.exit(1));\""
  },
  "keywords": ["mercadopago", "pagamento", "nodejs", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mercadopago": "^2.0.9",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
